{
    "name": "owenconti/owenconti.com",
    "type": "project",
    "description": "Owen Conti's personal website.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "archtechx/laravel-pages": "^0.3",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.48",
        "laravel/tinker": "^2.8",
        "laravel/vapor-cli": "^1.64",
        "laravel/vapor-core": "^2.37",
        "league/commonmark": "^2.4",
        "league/flysystem-aws-s3-v3": "^3.24",
        "lorisleiva/laravel-actions": "^2.8",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/once": "^3.1",
        "tightenco/ziggy": "^2.0",
        "torchlight/torchlight-commonmark": "^0.5.5",
        "torchlight/torchlight-laravel": "^0.5.14"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.51",
        "laravel/sail": "^1.25",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^3.9",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix --config .php-cs-fixer.dist.php",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
